<VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox id="navigation" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" styleClass="nav" VBox.vgrow="NEVER">
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" text="ESB selection" HBox.hgrow="ALWAYS">
                    <font>
                        <Font name="Segoe UI Light" size="24.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox nodeOrientation="LEFT_TO_RIGHT" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <children>
                <GridPane HBox.hgrow="ALWAYS">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints maxHeight="278.0" minHeight="10.0" prefHeight="120.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="458.0" minHeight="10.0" prefHeight="458.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label contentDisplay="CENTER" text="Select the ESB you want to get the Key Performance Indicators" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                            <font>
                                <Font name="Segoe UI Light" size="24.0" />
                            </font>
                        </Label>
                        <ComboBox id="esbChoiceBox" prefWidth="150.0" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="TOP" />
                    </children>
                </GridPane>
            </children>
        </HBox>
    </children>
</VBox>
